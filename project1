from tkinter import *


root = Tk()
root.title("Super woman captain awesomeness")
root.configure(background="purple")


def click():
    output.insert(END, "Dit is een hint. \n")
    Hint1done = Label(root, text="Hint 1").grid(row=0, column = 0)


Hint1 = Button(root, text="Hint 1", command = click).grid(row = 0, column = 0)

output = Text(root)
output.grid(row = 0, column = 1)



root.mainloop()

